{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 15
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": 0,
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}
